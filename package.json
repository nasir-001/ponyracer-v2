{
  "name": "ponyracer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e --browser firefox",
    "test:unit": "vue-cli-service test:unit",
    "vue-ninja": "vue-ninja",
    "eslint": "eslint",
    "format": "prettier --write \"{src,tests}/**/*.+(vue|ts|js|json|html|css)\"",
    "postinstall": "npm run format"
  },
  "dependencies": {
    "@vee-validate/i18n": "4.0.1",
    "@vee-validate/rules": "4.0.3",
    "axios": "0.21.1",
    "bootstrap": "4.6.0",
    "date-fns": "2.16.1",
    "font-awesome": "4.7.0",
    "vee-validate": "4.0.4",
    "vue": "3.0.4",
    "vue-router": "4.0.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.18",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "@vue/cli-plugin-e2e-cypress": "4.5.9",
    "@vue/cli-plugin-eslint": "4.5.9",
    "@vue/cli-plugin-typescript": "4.5.9",
    "@vue/cli-plugin-unit-jest": "4.5.9",
    "@vue/cli-service": "4.5.9",
    "@vue/compiler-sfc": "3.0.4",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "7.0.0",
    "@vue/test-utils": "2.0.0-beta.12",
    "cypress": "6.1.0",
    "eslint": "7.15.0",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-vue": "7.2.0",
    "jest-stare": "2.2.0",
    "mocha-spec-json-output-reporter": "1.1.7",
    "prettier": "2.2.1",
    "typescript": "4.1.2",
    "vue-jest": "5.0.0-alpha.7",
    "vue-ninja": "0.0.12"
  },
  "jest-stare": {
    "resultDir": "results",
    "resultJson": "jest-results.json",
    "coverageLink": "../coverage/lcov-report/index.html"
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none"
  }
}
